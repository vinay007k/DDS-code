#include <iostream>
using namespace std;

void fibonacci(int n) {
    int a = 0, b = 1, next;

    for (int i = 0; i < n; ++i) {
        cout << a << " ";
        next = a + b;
        a = b;
        b = next;
    }
    cout << endl;
}

int main() {
    int terms;
    cout << "Enter the number of terms: ";
    cin >> terms;

    cout << "Fibonacci sequence up to " << terms << " terms:" << endl;
    fibonacci(terms);

    return 0;
}
