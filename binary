#include <iostream>
using namespace std;
int main(){
    int arr[]={10,20,40,80,100};
    int n=sizeof(arr)/sizeof(arr[0]);
    int low=3; int index=-1; int key=100;
    int high= n;
    while (low<=high){
        int mid=(low+high)/2;
    if(arr[mid]=key){
        index=mid;
        break;
    }
    else 
    if(arr[mid]<key){
        low=mid+1;
        }
        else{
            high=mid-1;
        }
}
if(index==-1){
    cout<<"not found";
}
else{
    cout<<"found at index "<<index;
}
}
